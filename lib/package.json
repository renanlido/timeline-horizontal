{
  "name": "@reactuiutils/horizontal-timeline",
  "version": "1.0.0",
  "description": "A horizontal timeline component for React",
  "main": "dist/index.js",
  "style": "dist/index.css",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.tsx --format esm --clean --dts --external react --onSuccess \"node ./add-import-css.cjs\"",
    "dev": "tsup src/index.tsx --format esm --dts --external react --watch --onSuccess \"node ./add-import-css.cjs\""
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.17",
    "esbuild-css-modules-plugin": "^3.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.1"
  },
  "keywords": [
    "react",
    "timeline",
    "horizontal-timeline",
    "reacuikit"
  ],
  "repository": "https://github.com/renanlido/timeline-horizontal",
  "homepage": "https://github.com/renanlido/timeline-horizontal",
  "bugs": {
    "url": "https://github.com/renanlido/timeline-horizontal/issues"
  },
  "author": {
    "name": "Renan Oliveira",
    "email": "renanlido@yahoo.com.br",
    "url": "https://www.renanlido.me"
  },
  "license": "MIT"
}
